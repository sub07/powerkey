// Static tasks configuration.
//
// Example:
[
  {
    "label": "run",
    "command": "cargo",
    "args": ["run"],
    "env": {
      "RUST_LOG": "TRACE"
    }
  },
  {
    "label": "build",
    "command": "cargo",
    "args": ["build"]
  },
  {
    "label": "check",
    "command": "cargo",
    "args": ["check"]
  }
]
